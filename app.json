{
  "expo": {
    "name": "AppGeral",
    "slug": "AppGeral",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "width": "100%",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/euicon.png"
      },
      "package": "com.viniciusbl21.AppGeral"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
